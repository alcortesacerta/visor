package com.iecisa.sat.saie.service.rest.dto;

public class Verificacion1a1DTO {

	private String rfc;
	private Integer fingerId;
	private byte[] fingerData;
	
	public String getRfc() {
		return rfc;
	}
	public void setRfc(String rfc) {
		this.rfc = rfc;
	}
	public Integer getFingerId() {
		return fingerId;
	}
	public void setFingerId(Integer fingerId) {
		this.fingerId = fingerId;
	}
	public byte[] getFingerData() {
		return fingerData;
	}
	public void setFingerData(byte[] fingerData) {
		this.fingerData = fingerData;
	}
	
	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("Verificacion1a1DTO [");
		if (rfc != null) {
			builder.append("rfc=");
			builder.append(rfc);
			builder.append(", ");
		}
		if (fingerId != null) {
			builder.append("fingerId=");
			builder.append(fingerId);
			builder.append(", ");
		}
		if (fingerData != null) {
			builder.append("fingerData=");
			builder.append(fingerData.length);
		}
		builder.append("]");
		return builder.toString();
	}
	
}
