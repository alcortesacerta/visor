package com.iecisa.sat.saie.vf.integration.service.dto;

public enum Permisos {

	VISOR,//acceso al visor de documentos
	DES_BIOMETRICOS,//descarga de biometricos
	DES_DOCUMENTOS,//descarga de documentos
	CERTIFICAR,//certificar
	DUPLICIDAD,//acceso al visor de duplicados
	EXPUESTOS,//acceso a rfc's expuestos (ej. el rfc de Peña Nieto)
	VALIDAR,//permiso para realizar validaciones de actualizaciones
	AUTORIZAR,//permiso para realizar autorizaciones de actualizaciones
	REPORTE_BIO,//permisos para cargar reporte biometrico y poner comentarios en el visor de duplicidad
	VALIDA_NACIONAL //permiso para no aplicar el filtro de localidad a validacion y autorizacion (se necesita tener los otros 2 permisos también)
}
