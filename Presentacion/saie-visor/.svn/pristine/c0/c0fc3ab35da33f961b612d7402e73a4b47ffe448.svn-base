*{
	margin: 0px;
	padding: 0px;
	font-family: sans-serif;
	font-size: 20px;
	font-weight: bold;
	border: 0px;
}

body{
	background-color: white; 
	background-image: url(../imgs/bg.jpg); 
	background-repeat: repeat-y;
}

img {
	text-decoration: none;
	border: 0px;
}

a{	/*apariencia de ligas*/
	text-decoration: none;
	border: 0px;
	font-size: 14px;
	color: graytext;
	font-weight: normal;
}

a:visited{	/*apariencia de ligas visitadas*/
	text-decoration: none;
	font-size: 14px;
	color: graytext;
	font-weight: normal;
}


#head-titulo{
	height: 95px;
	width: 100%;
	background-image: url(../imgs/head_saie0.png);
	background-repeat: no-repeat;
	position: relative;
}

#logo-sat{
	position: absolute;
	right: 25px;
	top: 25px;
}

#menu{
	width: 300px; 
	position: absolute; 
	top: 30%; 
	right: 10%;
}

.boton{
	margin-top: 10px;
	float: right;
}

.holder{
	position: absolute;
	top: 40px;
	padding-left: 15px;
}

#logout{
	position:absolute;
	bottom: 20px;
	left: 20px;
	cursor: pointer;
}

.menuLabel{
	height: 75px;
}

#error-mensaje{
	position: absolute;
  	top: 40px;
  	text-align: center;
  	width: 100%;
}