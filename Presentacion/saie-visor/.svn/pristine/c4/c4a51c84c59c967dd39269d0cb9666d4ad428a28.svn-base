*{
	margin: 0px;
	padding: 0px;
	font-family: sans-serif;
	font-size: 20px;
	font-weight: bold;
	border: 0px;
}

body{
	background-color: white; 
	background-image: url(../imgs/bg.jpg); 
	background-repeat: repeat-y;
}

img {
	text-decoration: none;
	border: 0px;
}

a{	/*apariencia de ligas*/
	text-decoration: none;
	border: 0px;
	font-size: 14px;
	color: graytext;
	font-weight: normal;
}

a:visited{	/*apariencia de ligas visitadas*/
	text-decoration: none;
	font-size: 14px;
	color: graytext;
	font-weight: normal;
}


#head-titulo{
	height: 95px;
	width: 100%;
	background-image: url(../imgs/head_saie0.png);
	background-repeat: no-repeat;
	position: relative;
}

#logo-sat{
	position: absolute;
	right: 25px;
	top: 25px;
}

#errorHolder{
	text-align: center;
  	font-size: 30px;
  	margin-top: 50px;
  	margin-left: 50px;
  	margin-right: 50px;
}

button[type=button]{ /*estilo de boton*/
	height: 25px;
	min-width: 150px;
	background-color: #C10C2D;
	border: none;
	border-radius: 5px;
	color: white;
	font-size: 16px;
	cursor: pointer;
	margin: 10px;
}